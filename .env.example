# Database Configuration
DATABASE_URL=postgresql://localhost:5432/hedge_fund_tracker
DB_HOST=localhost
DB_PORT=5432
DB_NAME=hedge_fund_tracker
DB_USER=postgres
DB_PASSWORD=postgres

# API Configuration
API_PORT=3001
NODE_ENV=development
JWT_SECRET=your-jwt-secret-here

# LLM API Configuration
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# SEC EDGAR Configuration
SEC_USER_AGENT=your-company-name your-email@domain.com
SEC_API_RATE_LIMIT=10

# External API Keys (Optional)
SEC_API_IO_KEY=your-sec-api-io-key

# Frontend Configuration
VITE_API_URL=http://localhost:3001

# Ingestion Service Configuration
INGESTION_SCHEDULE_ENABLED=true
INGESTION_CRON_SCHEDULE=0 0 * * 0  # Every Sunday at midnight

# Logging Configuration
LOG_LEVEL=info
LOG_DIR=./logs

# Cache Configuration
ANALYSIS_CACHE_TTL_HOURS=24