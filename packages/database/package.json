{
  "name": "hedge-fund-tracker-database",
  "version": "1.0.0",
  "description": "Database schema and migrations for hedge fund tracker",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "migrate": "tsx src/migrate.ts",
