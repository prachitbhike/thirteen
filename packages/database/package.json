    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "drizzle-orm": "^0.29.0",
    "drizzle-kit": "^0.20.0",
    "postgres": "^3.4.3",
    "@supabase/supabase-js": "^2.38.0"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  }
}